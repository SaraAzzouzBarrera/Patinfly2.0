<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.scooter.ScooterDetailActivity">

    <LinearLayout
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/uuid"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="Uuid"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/ScooterName"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="Name"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/longScooter"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="Longitud"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/latScooter"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="Latitud"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/batteryLevel"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="Battery Level"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/mTravel"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="mTraveled"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/LastData"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="Last Data"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/state"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="State"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/onRent"
            android:layout_width="269dp"
            android:layout_height="46dp"
            android:text="On Rent"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:visibility="visible" />

        <Button
            android:id="@+id/Ok"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Ok" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
